{% for post in collections.lib %}
  <div class="article">
    <a href="{{ post.url }}">{{ post.data.title }}</a><br><br><hr>
    <br>
    <i>
      <h5 class="date">
        {{ post.data.author }} - {{ post.data.date | date: "%b %d, %Y" }}</h5>
    </i><br>
  </div>
{% endfor %}